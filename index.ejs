                                        <div class="card-block">
                                            <h6 class="mb-4">이번달 통화방 사용수</h6>
                                            <div class="row d-flex align-items-center">
                                                <div class="col-9">
                                                    <h3 class="f-w-300 d-flex align-items-center  m-b-0"><i class="feather icon-arrow-up text-c-green f-30 m-r-10"></i>$ 8.638.32</h3>
                                                </div>
                                                <div class="col-3 text-right">
                                                    <p class="m-b-0">저번달 대비 80%</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--[ year  sales section ] end-->
                                <!--[ Recent Users ] start-->
                                <div class="col-xl-8 col-md-6">
                                    <div class="card Recent-Users">
                                        <div class="card-header">
                                            <h5>서버 유저 목록</h5>
                                        </div>
                                        <div class="card-block px-0 py-3">
                                            <div class="table-responsive">
                                                <table class="table table-hover">
                                                    <tbody>
                                                        <% memberinfo.forEach((item, index) => { if (item.user.id === '751612891697774662') return %>
                                                            <tr class="unread">
                                                                <td><img class="rounded-circle" style="width:40px;" src="<%= item.user.displayAvatarURL() || item.user.defaultAvatarURL %>" alt="activity-user"></td>
                                                                <td>
                                                                    <h6 class="mb-1"><%= item.user.username %>#<span class="text-muted"><%= item.user.discriminator %></span></h6>
                                                                    <p class="m-0">Activities : <%= item.user.presence.activities[0] ? (item.user.presence.activities[0].state || 'None').substring(0, 25)  : 'None' %></p>
                                                                </td>
                                                                <td>
                                                                    <h6 class="text-muted">Status : <%= item.user.presence.status %> <i class="fas fa-circle text-c-<%= item.user.presence.status %> f-10 m-r-15"></i></h6>
                                                                </td>
                                                                <td><a href="/api/kick?user=<%= item.user.id %>&guild=<%= item.guild.id %>" class="label theme-bg2 text-white f-0">Kick</a><a href="/api/ban?user=<%= item.user.id %>&guild=<%= item.guild.id %>" class="label theme-bg text-white f-0">Ban</a></td>
                                                            </tr>
                                                        <% }) %>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--[ Recent Users ] end-->

                                <!-- [ statistics year chart ] start -->
                                <div class="col-xl-4 col-md-6">
                                    <div class="card card-event">
                                        <div class="card-block">
                                            <div class="row align-items-center justify-content-center">
                                                <div class="col">
                                                    <h5 class="m-0">서버 온라인 유저 수</h5>
                                                </div>
                                                <div class="col-auto">
                                                    
                                                </div>
                                            </div>
                                            <h2 class="mt-3 f-w-300"><%= memberinfo.filter((member) => member.presence.status !== 'offline').length %> <sub class="text-muted f-14">Online</sub><label class="label theme-bg2 text-white f-14 f-w-400 ml-2"><%= Math.floor(memberinfo.filter((member) => member.presence.status !== 'offline').length / memberinfo.length * 100) %>%</label></h2>
                                            <i class="fas fa-plug text-c-purple f-50"></i>
                                        </div>
                                    </div>
                                    <div class="card">
                                        <div class="card-block border-bottom">
                                            <div class="row d-flex align-items-center">
                                                <div class="col-auto">
                                                    <i class="fas fa-users f-30 text-c-green"></i>
                                                </div>
                                                <div class="col">
                                                    <h3 class="f-w-300"><%= memberinfo.length %></h3>
                                                    <span class="d-block text-uppercase">서버 총 인원</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card-block">
                                            <div class="row d-flex align-items-center">
                                                <div class="col-auto">
                                                    <i class="fas fa-robot f-30 text-c-blue"></i>
                                                </div>
                                                <div class="col">
                                                    <h3 class="f-w-300"><%= memberinfo.filter((m) => m.user.bot).length %></h3>
                                                    <span class="d-block text-uppercase">서버 봇수</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- [ Main Content ] end -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
